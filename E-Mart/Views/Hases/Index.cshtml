@model IEnumerable<E_Mart.Models.Has>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Product.ProductName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Seller.SellerName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Product.ProductName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Seller.SellerName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.HasID }) |
            @Html.ActionLink("Details", "Details", new { id=item.HasID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.HasID })
        </td>
    </tr>
}

</table>
