@model IEnumerable<E_Mart.Models.Seller>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.SellerName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SellerEmail)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SellerPassword)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SellerPhone)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SellerAdress)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SellerRating)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.SellerName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SellerEmail)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SellerPassword)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SellerPhone)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SellerAdress)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SellerRating)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.SellerID }) |
            @Html.ActionLink("Details", "Details", new { id=item.SellerID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.SellerID })
        </td>
    </tr>
}

</table>
